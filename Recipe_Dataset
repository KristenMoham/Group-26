   "source": [
    "#Shell Sort\n",
    "n = len(mIng)\n",
    "gap = n//2\n",
    "while gap > 0:\n",
    "        for i in range(gap, n):\n",
    "            temp = mIng[i]\n",
    "            j = i            \n",
    "            temp_ingredient = temp[1][2:10]\n",
    "            prev = mIng[j - gap]\n",
    "            prev_ingredient = prev[1][2:10]\n",
    "            while(j >= gap and prev_ingredient > temp_ingredient):\n",
    "                prev = mIng[j - gap]\n",
    "                prev_ingredient = prev[1][2:10]\n",
    "                mIng[j] = prev\n",
    "                j -= gap\n",
    "            mIng[j] = temp\n",
    "            \n",
    "        if(gap == 2):\n",
    "            gap = 1\n",
    "        else:\n",
    "            gap //= 2\n",
    "        print(gap)\n",
    "\n",
    "print(mIng[1][1])\n",
    "print(mIng[2][1])\n",
    "print(mIng[3][1])\n",
    "print(mIng[4][1])\n",
    "print(mIng[5][1])\n",
    "print(mIng[6][1])\n",
    "print(mIng[7][1])\n",
    "print(mIng[8][1])\n",
    "print(mIng[9][1])"
   ]
  }
